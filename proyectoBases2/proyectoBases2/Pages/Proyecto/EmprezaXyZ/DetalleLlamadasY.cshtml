@page
@model proyectoBases2.Pages.Proyecto.EmprezaXyZ.DetalleLlamadasYModel
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="es">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Tabla detalles llamadas empresa Y</title>
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css">
</head>

<body>
    <div class="container">
        <h2>Tabla detalles llamadas empresa Y</h2>
        <table class="table">
            <thead>
                <tr>
                    <th>Numero de telefono que inicio llamada</th>
                    <th>Numero de telefono que recibe llamada</th>
                    <th>Tipo llamada</th>
                    <th>Hora inicio</th>
                    <th>Hora fin</th>
                    <th>Cantidad de minutos</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model.detallesLlamadas)
                {
                    <tr>
                        <td>@item.NumeroDe</td>
                        <td>@item.NumeroA</td>
                        <td>@item.TipoLlamada</td>
                        <td>@item.HoraInicio</td>
                        <td>@item.HoraFin</td>
                        <td>@item.Duracion</td>
                    </tr>
                }
            </tbody>

        </table>

        <div class="row mb-3">
            <div class="col-sm-3 d-grid">
                <a class="btn btn-outline-primary" href="/Proyecto/EmprezaXyZ/IndexY?Numero=@Model.Numero" role="button">Regresar</a>
            </div>
        </div>
    </div>
</body>
</html>

